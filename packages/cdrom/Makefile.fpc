#
#   Makefile.fpc for cdrom
#

[package]
name=cdrom
version=2.2.1rc1

[target]
units=cdrom discid
implicitunits_win32=scsidefs wnaspi32 cdromioctl wincd
implicitunits_linux=major lincd
examples=showcds getdiscid

[install]
fpcpackage=y

[compiler]
includedir=src
sourcedir=src tests

[default]
fpcdir=../..

[rules]
.NOTPARALLEL:
