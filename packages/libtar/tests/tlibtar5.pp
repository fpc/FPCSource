    { === PART 3: Summary === }
    WriteLn('=== Summary ===');
    WriteLn;
    WriteLn('Total entries: ', EntryCount);

    if EntryCount <> TotalEntries then
    begin
        WriteLn('ERROR: Expected ',TotalEntries,' entries!');
        Inc(Errors);
    end;

    if Errors = 0 then
        WriteLn('All tests PASSED.')
    else
        WriteLn('FAILED with ', Errors, ' error(s).');
  finally
    DeleteFile(TAR_FILENAME);
  end;

  WriteLn;

  Halt(Errors);
end.
