#
#   Makefile.fpc for Apple Universal Interfaces for Free Pascal
#

[package]
name=univint
version=3.2.3

[target]
units=iosxlocale iosxwstr

[libs]
libversion=2.0.0

# FPCMacOSAll is not a build unit, but directly includes the code of
# most other units
[install]
fpcpackage=y

[compiler]
includedir=src
sourcedir=src

[default]
fpcdir=../..

[prerules]

[rules]
