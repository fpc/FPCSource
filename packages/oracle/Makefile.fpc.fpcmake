#
#  Makefile.fpc for oracle units
#

[package]
name=oracle
version=3.2.2

[target]
units=oci ocidyn oratypes oraoci
exampledirs=examples

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests

# in libdir variable must be path to libraries
# libclntsh.*,libnlsrtl3.*
#[compiler]
#libdir=/usr/local/oracle8cli/lib

[shared]
build=n

[rules]
.NOTPARALLEL:

