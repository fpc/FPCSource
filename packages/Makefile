zlib_shared:
	$(MAKE) -C zlib shared

zlib_showinstall:
	$(MAKE) -C zlib showinstall

zlib_install:
	$(MAKE) -C zlib install

zlib_sourceinstall:
	$(MAKE) -C zlib sourceinstall

zlib_zipinstall:
	$(MAKE) -C zlib zipinstall

zlib_zipinstalladd:
	$(MAKE) -C zlib zipinstalladd

zlib_clean:
	$(MAKE) -C zlib clean

zlib_cleanall:
	$(MAKE) -C zlib cleanall

zlib_depend:
	$(MAKE) -C zlib depend

zlib_info:
	$(MAKE) -C zlib info
endif

