#
#   Makefile.fpc for gdbm bindings
#

[package]
name=gdbm
version=1.0.6

[target]
units=gdbm
examples=testgdbm testgdbm2

[require]
package=rtl

[install]
examplesubdir=gdbm
fpcpackage=y

[default]
fpcdir=../../..

[rules]
testgdbm$(EXEEXT): testgdbm.pp gdbm$(PPUEXT)

testgdbm2$(EXEEXT): testgdbm2.pp gdbm$(PPUEXT)
