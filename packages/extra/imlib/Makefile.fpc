#
#   Makefile.fpc for Free Pascal ImLib 1.x Packages
#

[package]
name=imlib
version=2.0.0

[target]
units=gdk_imlib imlib

[require]
libc=y
packages=gtk x11

[compiler]
sourcedir=imlib gdk_imlib

[install]
fpcpackage=y
fpcsubdir=packages

[default]
fpcdir=../../..

[shared]
build=n

[rules]
.NOTPARALLEL:
