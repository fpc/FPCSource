#
#   Makefile.fpc for SDL bindings
#

[package]
name=os2units
version=1.0.0

[require]
libc=y

[target]
units=buildall clkdll clktest dive ftpapi hwvideo lvm mciapi mcidrv mci mmbase mmio som sw wpstk

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests examples

[rules]
.NOTPARALLEL:
