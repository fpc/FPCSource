#
#   Makefile.fpc for Users  bindings
#

[package]
name=users
version=2.2.3

[target]
units=grp pwd users
units_linux=shadow crypth
examples=testuser testpass2
examples_linux=testpass

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests examples

[shared]
build=n

[rules]
.NOTPARALLEL:
