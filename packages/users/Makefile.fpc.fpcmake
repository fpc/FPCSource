[package]
name=users
version=3.2.4-rc1

[target]
units=grp pwd users
units_linux=shadow crypth
exampledirs=examples

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src

[shared]
build=n

[rules]
.NOTPARALLEL:
