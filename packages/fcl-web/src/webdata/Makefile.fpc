#
#   Makefile.fpc for FCL Web components
#

[package]
main=fcl-web
version=3.2.2

[target]
units=fpwebdata sqldbwebdata fpextjs extjsjson extjsxml

[require]
packages=fcl-base fcl-xml fcl-db fcl-json fcl-process

[compiler]
options=-S2h

[install]
fpcpackage=y

[default]
fpcdir=../../../..


[rules]
.NOTPARALLEL:
