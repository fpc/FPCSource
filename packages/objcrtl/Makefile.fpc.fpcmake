#
#   Makefile.fpc for objective-c runtime library bindings
#

[package]
name=obcjrtl
version=3.2.4-rc1

[target]
units=objcrtl objcrtl10 objcrtl20 objcrtliphoneos objcrtlmacosx objcrtlutils

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src

[rules]
.NOTPARALLEL:
