#
#   Makefile.fpc for Postgres bindings
#

[package]
name=postgres
version=2.0.0

[target]
units=dllist dllistdyn postgres postgres3 postgres3dyn
examples=testpg1 testpg2

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src tests examples

[shared]
build=n

[rules]
.NOTPARALLEL:

