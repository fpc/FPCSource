#
#   Makefile.fpc for nds units
#

[package]
name=libndsfpc
version=2.2.4a

[target]
units=nds9 nds7 dswifi9 dswifi7 fat gbfs filesystem maxmod9  maxmod7
exampledirs=examples

[compiler]
options=
includedir=src src/dswifi src/fat src/maxmod
sourcedir=src tests

[prerules]
BINUTILSPREFIX=arm-eabi-

[install]
fpcpackage=y

[default]
cpu=arm
target=nds
fpcdir=../..

[require]
nortl=y

[shared]
build=n

[rules]
.NOTPARALLEL:

